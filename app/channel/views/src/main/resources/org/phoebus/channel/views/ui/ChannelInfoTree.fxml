<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.phoebus.channel.views.ui.ChannelInfoTreeController">
   <children>
      <TreeTableView fx:id="treeTable" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <columns>
          <TreeTableColumn fx:id="name" minWidth="100.0" prefWidth="-1.0" text="Channel" />
          <TreeTableColumn fx:id="value" minWidth="100.0" prefWidth="-1.0" />
        </columns>
      </TreeTableView>
   </children>
</AnchorPane>
